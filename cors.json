[
  {
    "origin": [
      "http://localhost:*",
      "http://127.0.0.1:*",
      "https://*.web.app",
      "https://*.firebaseapp.com"
    ],
    "method": [
      "GET",
      "HEAD",
      "PUT",
      "POST",
      "DELETE",
      "OPTIONS"
    ],
    "maxAgeSeconds": 3600,
    "responseHeader": [
      "Content-Type",
      "Content-Length",
      "Cache-Control",
      "Access-Control-Allow-Origin",
      "Access-Control-Allow-Credentials",
      "Authorization"
    ]
  }
]
